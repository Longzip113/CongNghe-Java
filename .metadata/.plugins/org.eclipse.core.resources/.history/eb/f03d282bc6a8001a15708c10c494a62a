package com.longnguyen.model;

public class EmployeeModel {
	private String maPB;
	private String tenPB;
	private String sdtKh;
	private DepartmentModel nhom;
	
	
	public EmployeeModel(String maKh, String tenKh, String sdtKh, String gmail) {
		super();
		this.maKh = maKh;
		this.tenKh = tenKh;
		this.sdtKh = sdtKh;
	}
	
	public String getMaKh() {
		return maKh;
	}
	public void setMaKh(String maKh) {
		this.maKh = maKh;
	}
	public String getTenKh() {
		return tenKh;
	}
	public void setTenKh(String tenKh) {
		this.tenKh = tenKh;
	}
	public String getSdtKh() {
		return sdtKh;
	}
	public void setSdtKh(String sdtKh) {
		this.sdtKh = sdtKh;
	}
	public String getGmail() {
		return gmail;
	}
	public void setGmail(String gmail) {
		this.gmail = gmail;
	}

	public DepartmentModel getNhom() {
		return nhom;
	}

	public void setNhom(DepartmentModel nhom) {
		this.nhom = nhom;
	}
	
	@Override
	public String toString() {
		// TODO Auto-generated method stub
		return this.tenKh;
	}
}
