package com.longnguyen.model;

public class EmployeeModel {
	private String maNV;
	private String tenNV;
	private String luongNV;
	private DepartmentModel PB;

	public String getMaNV() {
		return maNV;
	}

	public void setMaNV(String maNV) {
		this.maNV = maNV;
	}

	public String getTenNV() {
		return tenNV;
	}

	public void setTenNV(String tenNV) {
		this.tenNV = tenNV;
	}

	public String getLuongNV() {
		return luongNV;
	}

	public void setLuongNV(String luongNV) {
		this.luongNV = luongNV;
	}

	public DepartmentModel getPB() {
		return PB;
	}

	public void setPB(DepartmentModel pB) {
		PB = pB;
	}

	public EmployeeModel(String maNV, String tenNV, String luongNV, DepartmentModel pB) {
		super();
		this.maNV = maNV;
		this.tenNV = tenNV;
		this.luongNV = luongNV;
		PB = pB;
	}

	@Override
	public String toString() {
		// TODO Auto-generated method stub
		return this.tenNV;
	}
}
